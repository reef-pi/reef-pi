{
  "name": "reef-pi",
  "version": "1.0.0",
  "description": "A Raspberry Pi based reeftank automation system",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "devDependencies": {
    "@babel/core": "7.10.4",
    "@babel/plugin-transform-modules-commonjs": "7.13.8",
